// NATURAL NUMBER:
int n=sc.nextInt();
int sum=0;
for(int i=0;i<=n;i++) {
	sum= sum+i;
}
System.out.println("sum of natural number:" + sum);
// CUBE	
int a = sc.nextInt();
int cube=0;
for(int i=1;i<=a;i++) {
	cube =i*i*i;

System.out.println("cube is "+ cube);
}
